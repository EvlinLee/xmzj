<?xml version="1.0" encoding="utf-8"?>
<inset android:insetLeft="1.0px" android:insetRight="1.0px"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <solid android:color="#ff999999" />
                <corners android:topLeftRadius="12.0dip" android:topRightRadius="12.0dip" android:bottomLeftRadius="12.0dip" android:bottomRightRadius="12.0dip" />
                <stroke android:width="0.5dip" android:color="#ff999999" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="@color/white" />
                <corners android:topLeftRadius="12.0dip" android:topRightRadius="12.0dip" android:bottomLeftRadius="12.0dip" android:bottomRightRadius="12.0dip" />
                <stroke android:width="0.5dip" android:color="#ff999999" />
            </shape>
        </item>
    </selector>
</inset>